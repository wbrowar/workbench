{# @var craft \craft\web\twig\variables\CraftVariable #}
{# @var entry \craft\elements\Entry #}

{# Allow only visitors that are logged in or visitors visiting from whitelisted IP addresses #}
{% do checkAccessByIp() %}

<!DOCTYPE html>

{% minify %}
  <html class="no-js" lang="en">
    <head>
<%- include('_ejs/_head_meta') %>
      {{ adminbar() }}

<%- include('_ejs/_head_scripts') %>

<%- include('_ejs/_head_tracking') %>
    </head>
    <body>
      {% minify %}
      <div id="page">
        {% block header %}
          {% include 'partials/_ui' %}
        {% endblock %}

        <section class="page_wrapper">
          {% block contentBefore %}{% endblock %}
          {% block content %}{% endblock %}
          {% block contentAfter %}{% endblock %}
        </section>

        {% block footer %}
          {% include 'partials/_footer' %}
        {% endblock %}
      </div>
      {% endminify %}

      {% block jsFooter %}
<%- include('_ejs/_body_load_js') %>
      {% endblock %}
    </body>
  </html>
{% endminify %}